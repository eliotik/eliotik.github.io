{
    "name": "eliotik.github.io",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro check && astro build && jampack ./dist",
        "preview": "astro preview",
        "astro": "astro",
        "sync": "astro sync",
        "format:check": "prettier --plugin-search-dir=. --check .",
        "format": "prettier --plugin-search-dir=. --write .",
        "prepare": "husky install",
        "lint": "eslint .",
        "clean": "./clean-cache.sh",
        "fresh": "./clean-cache.sh && yarn dev"
    },
    "engines": {
        "node": "18.19.0"
    },
    "dependencies": {
        "@astrojs/check": "0.5.6",
        "@astrojs/mdx": "2.1.1",
        "@astrojs/partytown": "^2.1.4",
        "@astrojs/react": "3.0.10",
        "@astrojs/rss": "4.0.5",
        "@astrojs/sitemap": "3.1.1",
        "@astrojs/tailwind": "5.1.0",
        "@astrojs/ts-plugin": "1.5.3",
        "@expressive-code/plugin-collapsible-sections": "0.33.4",
        "@expressive-code/plugin-line-numbers": "0.33.4",
        "@resvg/resvg-js": "2.6.0",
        "@tailwindcss/typography": "0.5.10",
        "astro": "4.4.9",
        "astro-expressive-code": "0.33.4",
        "flowbite": "2.3.0",
        "flowbite-react": "0.7.2",
        "fuse.js": "7.0.0",
        "github-slugger": "2.0.0",
        "remark-collapse": "0.1.2",
        "remark-toc": "9.0.0",
        "satori": "0.10.13",
        "sharp": "0.32.6",
        "tailwindcss": "3.4.1",
        "typescript": "5.3.3"
    },
    "devDependencies": {
        "@divriots/jampack": "0.23.3",
        "@types/github-slugger": "2.0.0",
        "@types/react": "18.2.61",
        "@typescript-eslint/eslint-plugin": "7.1.0",
        "@typescript-eslint/parser": "7.1.0",
        "astro-eslint-parser": "0.16.3",
        "eslint": "8.57.0",
        "eslint-plugin-astro": "0.31.4",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "husky": "9.0.11",
        "lint-staged": "15.2.2",
        "prettier": "3.2.5",
        "prettier-plugin-astro": "0.13.0",
        "prettier-plugin-tailwindcss": "0.5.11",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,md,mdx,json}": [
            "prettier --plugin-search-dir=. --write"
        ]
    },
    "resolutions": {
        "sharp": "0.32.6"
    }
}
